{
  "name": "react-native-mathjax-html-to-svg",
  "version": "1.1.0",
  "description": "React Native component to display mathematics in html using MathJax. Uses [MathJax](https://github.com/mathjax/)",
  "repository": {
    "type": "git",
    "url": "git@github.com:webyonet/react-native-mathjax-html-to-svg.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run copy:mathjax",
    "copy:mathjax": "cp -r mathjax dist/",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "watch": "tsc --watch"
  },
  "keywords": [
    "react-native",
    "react-native-mathjax",
    "react-native-mathjax-html-to-svg",
    "mathjax-html-to-svg",
    "mathjax",
    "svg",
    "mathjax-svg"
  ],
  "author": {
    "name": "jurajbilansky",
    "email": "jurajbilansky@gmail.com"
  },
  "dependencies": {
    "html-entities": "^2.4.0",
    "mhchemparser": "^4.2.1",
    "speech-rule-engine": "^5.0.0-beta.3"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "homepage": "https://github.com/webyonet/react-native-mathjax-html-to-svg.git",
  "license": "MIT"
}
